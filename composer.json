{
    "name": "jantinnerezo/laravel-model-meta",
    "description": "",
    "keywords": [
        "jantinnerezo",
        "laravel-model-meta"
    ],
    "homepage": "https://github.com/jantinnerezo/laravel-model-meta",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jantinn Erezo",
            "email": "erezojantinn@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0 || ^4.0 || ^5.0",
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jantinnerezo\\LaravelModelMeta\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jantinnerezo\\LaravelModelMeta\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
